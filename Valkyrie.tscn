[sub_resource type="GDScript" id="GDScript_nc525"]
script/source = "extends Sprite3D

@export var block_reference: CanvasLayer
var connections_revealed: bool = false


func reveal():
	
	if not visible:
		show()
		return
	
	if not connections_revealed:
		reveal_connections()
		return
	
	if block_reference.has_method(\"reveal\"):
		block_reference.reveal()


func reveal_connections():
	
	$Connections.show()
	var tween: Tween = get_tree().create_tween().set_parallel(true)
	tween.tween_property($\"../Camera\", \"position\", $\"../Anchors/von_Stauffenberg_Connections\".position, 5)
	tween.tween_property($\"../Camera\", \"rotation\", $\"../Anchors/von_Stauffenberg_Connections\".rotation, 5)
	connections_revealed = true
"

