[sub_resource type="GDScript" id="GDScript_vqg68"]
script/source = "extends Node3D

@onready var focused_on: Sprite3D = $Assassination
@onready var Camera: Camera3D = $Camera
@export var ignore_input: bool = false
@export var play_intro: bool = true
var start: bool = true



func get_destination() -> Marker3D:
	
	if Input.is_action_just_pressed(\"38\"):
		focused_on = $\"38-39\"
		return $\"Anchors/38\"
	if Input.is_action_just_pressed(\"39\"):
		focused_on = null
		return $\"Anchors/39\"
	if Input.is_action_just_pressed(\"41\"):
		focused_on = $\"41\"
		return $\"Anchors/41\"
	if Input.is_action_just_pressed(\"42\"):
		focused_on = $\"42\"
		return $\"Anchors/42\"
	if Input.is_action_just_pressed(\"43\"):
		focused_on = $\"43\"
		return $\"Anchors/43\"
	if Input.is_action_just_pressed(\"44\"):
		focused_on = $Assassination
		return $\"Anchors/44\"
	
	if Input.is_action_just_pressed(\"Q1\"):
		focused_on = $\"43\"
		return $\"Anchors/43_Q1\"
	if Input.is_action_just_pressed(\"Q2\"):
		focused_on = $\"43\"
		return $\"Anchors/43_Q2\"
	if Input.is_action_just_pressed(\"Q3\"):
		focused_on = $\"43\"
		return $\"Anchors/43_Q3\"
	if Input.is_action_just_pressed(\"Q4\"):
		focused_on = $\"43\"
		return $\"Anchors/43_Q4\"
	
	if Input.is_action_just_pressed(\"Hitler\"):
		focused_on = $Assassination
		return $\"Anchors/Hitler\"
	if Input.is_action_just_pressed(\"Stauffenberg\"):
		focused_on = $von_Stauffenberg
		return $Anchors/von_Stauffenberg
	if Input.is_action_just_pressed(\"Assassination\"):
		focused_on = $Assassination
		return $Anchors/Assassination
	if Input.is_action_just_pressed(\"Baddies\"):
		focused_on = $Assassination
		return $Anchors/Assassination_Baddies
	if Input.is_action_just_pressed(\"Goodies\"):
		focused_on = $Assassination
		return $Anchors/Assassination_Goodies
	if Input.is_action_just_pressed(\"Overview\"):
		focused_on = null
		return $\"Anchors/Overview\"
	
	return null



func _input(_event: InputEvent) -> void:
	
	if ignore_input:
		return
	
	if Input.is_action_just_pressed(\"MOUSE_CLICK\") and start and play_intro:
		$AnimationPlayer.play(\"Intro\")
		start = false
	
	if Input.is_action_just_pressed(\"MOUSE_CLICK\") && focused_on != null:
		focused_on.reveal()
	
	if not Input.is_anything_pressed():
		return
	
	var go_to: Marker3D = get_destination()
	if go_to == null:
		return
	
	var tween: Tween = get_tree().create_tween().set_parallel(true)
	Camera.idle_rotate = false
	tween.tween_property(Camera, \"position\", go_to.position, 1)
	tween.tween_property(Camera, \"rotation\", go_to.rotation, 1)
	$Camera/Timer.start()
"

